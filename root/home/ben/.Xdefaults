urxvt*foreground: #f2f2f2
urxvt*background: #101010

! Normal
! color0    black
! color1    red
! color2    green
! color3    yellow
! color4    blue
! color5    purple
! color6    cyan
! color7    white

urxvt*color0:    #6c6c6c
urxvt*color1:    #e9897c
urxvt*color2:    #b6e77d
urxvt*color3:    #ecebbe
urxvt*color4:    #a9cdeb
urxvt*color5:    #ea96eb
urxvt*color6:    #c9caec
urxvt*color7:    #f2f2f2

! Light
! color8    black
! color9    red
! color10    green
! color11    yellow
! color12    blue
! color13    purple
! color14    cyan
! color15    white

urxvt*color8:    #747474
urxvt*color9:    #f99286
urxvt*color10:    #c3f786
urxvt*color11:    #fcfbcc
urxvt*color12:    #b6defb
urxvt*color13:    #fba1fb
urxvt*color14:    #d7d9fc
urxvt*color15:    #e2e2e2

urxvt*scrollBar: false
#urxvt*internalBorder: 6

# Default font
urxvt*font: xft:Droid Sans Mono Slashed:pixelsize=12:antialias:true:hinting:true

# Adjust font size
# Ctrl-0  -  normal
# Ctrl-minus - small
# Ctrl-equals - large

urxvt.keysym.C-0: command:\033]710;xft:Droid Sans Mono Slashed:pixelsize=12:antialias:true:hinting:true\007
urxvt.keysym.C-minus: command:\033]710;xft:Droid Sans Mono Slashed:pixelsize=10:antialias:true:hinting:true\007
urxvt.keysym.C-equal: command:\033]710;xft:Droid Sans Mono Slashed:pixelsize=14:antialias:true:hinting:true\007


xterm*Foreground: white
xterm*Background: black
xterm*scrollBar: false
